{
  "name": "@proodos/api",
  "version": "1.0.0",
  "main": "dist/Server.js",
  "types": "dist/Server.d.ts",
  "dependencies": {
    "@proodos/application": "1.0.0",
    "express": "^4.22.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/Server.ts",
    "build": "tsc -p .",
    "start": "node dist/Server.js",
    "start:with-coverage": "npm run test:coverage --workspace Tests && npm run start"
  }
}
